{
  "family": "trackmymeal-container-final",
  "containerDefinitions": [
    {
      "name": "trackmymealcontainer",
      "image": "979466753238.dkr.ecr.eu-central-1.amazonaws.com/trackmymeal:latest",
      "cpu": 1024,
      "memory": 512,
      "memoryReservation": 256,
      "portMappings": [
        {
          "name": "trackmymealcontainer-8080-tcp",
          "containerPort": 8080,
          "hostPort": 80,
          "protocol": "tcp",
          "appProtocol": "http"
        },
        {
          "name": "trackmymealcontainer-8080-tcp",
          "containerPort": 8443,
          "hostPort": 443,
          "protocol": "tcp",
          "appProtocol": "https"
        }
      ],
      "essential": true,
      "environment": [],
      "environmentFiles": [],
      "mountPoints": [],
      "volumesFrom": [],
      "secrets": [
        {
          "name": "APP_URL",
          "valueFrom": "arn:aws:secretsmanager:eu-central-1:979466753238:secret:container/trackmymeal/environmentvariables-d4py9C:APP_URL::"
        },
        {
          "name": "GOOGLE_CLIENT_ID",
          "valueFrom": "arn:aws:secretsmanager:eu-central-1:979466753238:secret:container/trackmymeal/environmentvariables-d4py9C:GOOGLE_CLIENT_ID::"
        },
        {
          "name": "GOOGLE_CLIENT_SECRET",
          "valueFrom": "arn:aws:secretsmanager:eu-central-1:979466753238:secret:container/trackmymeal/environmentvariables-d4py9C:GOOGLE_CLIENT_SECRET::"
        },
        {
          "name": "MONGODB_URI",
          "valueFrom": "arn:aws:secretsmanager:eu-central-1:979466753238:secret:container/trackmymeal/environmentvariables-d4py9C:MONGODB_URI::"
        },
        {
          "name": "OPEN-FOOD-FACTS-API-URL",
          "valueFrom": "arn:aws:secretsmanager:eu-central-1:979466753238:secret:container/trackmymeal/environmentvariables-d4py9C:OPEN-FOOD-FACTS-API-URL::"
        },
        {
          "name": "CERT",
          "valueFrom": "arn:aws:secretsmanager:eu-central-1:979466753238:secret:container/trackmymeal/environmentvariables-d4py9C:CERT::"
        },
        {
          "name": "CERT_KEY",
          "valueFrom": "arn:aws:secretsmanager:eu-central-1:979466753238:secret:container/trackmymeal/environmentvariables-d4py9C:CERT_KEY::"
        }
      ],
      "ulimits": [],
      "logConfiguration": {
        "logDriver": "awslogs",
        "options": {
          "awslogs-create-group": "true",
          "awslogs-group": "/ecs/trackmymeal-container-final",
          "awslogs-region": "eu-central-1",
          "awslogs-stream-prefix": "ecs"
        },
        "secretOptions": []
      },
      "systemControls": []
    }
  ],
  "executionRoleArn": "arn:aws:iam::979466753238:role/ecsTaskExecutionRole-TrackMyMeal",
  "requiresCompatibilities": [
    "EC2"
  ],
  "cpu": "1024",
  "memory": "512",
  "runtimePlatform": {
    "cpuArchitecture": "X86_64",
    "operatingSystemFamily": "LINUX"
  }
}